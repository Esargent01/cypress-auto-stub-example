{
  "name": "cypress-auto-stub-example",
  "version": "1.0.0",
  "description": "Example project to demonstrate how to record/replay API with Cypress.",
  "license": "MIT",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^2.0.1",
    "concurrently": "^3.6.1",
    "cypress": "^3.1.0",
    "http-server": "^0.11.1",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:open:record": "cypress open --env autoRecordEnabled=1",
    "cy:run": "cypress run",
    "serve": "http-server -p 3456",
    "start": "concurrently \"npm run serve\" \"npm run cy:open\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
