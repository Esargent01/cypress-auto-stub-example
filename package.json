{
  "name": "cypress-pollyjs-plugin",
  "version": "1.0.0",
  "description": "A Cypress plugin to record, replay, and stub HTTP interactions with Pollyjs",
  "main": "index.js",
  "author": "Wenbo Jie <pinkyjie.gn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@pollyjs/adapter-fetch": "^1.0.1",
    "@pollyjs/adapter-xhr": "^1.0.1",
    "@pollyjs/core": "^1.1.0",
    "@pollyjs/node-server": "^1.0.0",
    "@pollyjs/persister-rest": "^1.0.1"
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "1.13.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
