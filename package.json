{
  "name": "cypress-auto-stub-example",
  "version": "1.0.0",
  "description": "Example project to demonstrate how to record/replay API with Cypress.",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.6.1",
    "cypress": "^3.0.2",
    "http-server": "^0.11.1",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  },
  "scripts": {
    "cypress": "cypress open",
    "serve": "http-server -p 3456",
    "start": "concurrently \"npm run serve\" \"npm run cypress\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
